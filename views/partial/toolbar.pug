div(class='pure-u-1 pure-u-md-1-8 gdit-toolbar')
    section(class='account__panel')
        img(class='account_img' src='/images/avata.jpg')
        h2 #{user.username}
        div(class='pure-g')
            div(class='pure-u-1-2')
                a(href='/admin/profile') Profile
            div(class='pure-u-1-2')
                a(href='/admin/logout') Logout
    section(class='gdit__panel')
        h1(class='gdit_title') Group
        ul(class='gdit_list group_list')
            each val in rooms
                li(data-id=val._id)
                    a(href='javascript:void(0)' class='room_item' id='room_item') #{val.name}

    section(class='gdit__panel')
        h1(class='gdit_title') Users
        ul(class='gdit_list users_list')

    a(class='btn-group-plus' href='/admin/create-room') Add Room
